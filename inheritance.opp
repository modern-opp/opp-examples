class Animal is
    method makeSound() : String

    method move() : String
end

class Bird extends Animal is
    method makeSound() : String is
        return "Chirp!"
    end

    method fly() : String is
        return "Flying..."
    end
end

class Eagle extends Bird is
    method makeSound() : String is
        return "Screech!"
    end

    method hunt() : String is
        return "Hunting..."
    end
end
